        Title="SvgImages" Height="300" Width="300" Loaded="Window_OnLoaded" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
