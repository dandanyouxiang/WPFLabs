        <TextBlock x:Name="Msg" Grid.Row="0" Margin="20" />
        <Grid x:Name="Container" Grid.Row="1">
            <!--<Image Source="{Binding SvgPath, Converter={svgc:SvgImageConverter}}" />-->
            <!--<Image Source="{svgc:SvgImage Source=ImagePerformanceDemo/Resources/timer.svg}" />-->
        </Grid>
    </Grid>
